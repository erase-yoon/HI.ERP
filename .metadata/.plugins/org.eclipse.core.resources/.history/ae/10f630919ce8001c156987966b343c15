		System.out.println("정답 17 : " + result);
		System.out.println("---------------------------------------------");
		
		
		//-------------------------------------------
		// <정답18> 덜 무식한 방법
		//-------------------------------------------
		switch(avg1) {
		case 100 :		case 99 :		case 98 :		case 97 :		case 96 :
		case 95 :		case 94 :		case 93 :		case 92 :		case 91 :
		case 90 :		case 89 :		case 88 :		case 87 :		case 86 :
		case 85 :		case 84 :		case 83 :		case 82 :		case 81 :
		case 80 : result = "합격"; break;
		case 79 : 		case 78 :		case 77 :		case 76 :		case 75 :
		case 74 : 		case 73 : 		case 72 : 		case 71 :		case 70 : 
		case 69 : 		case 68 : 		case 67 : 		case 66 : 		case 65 : 
		case 64 : 		case 63 : 		case 62 : 		case 61 : 		case 60 : 
		case 59 : 		case 58 : 		case 57 : 		case 56 : 		case 55 : 
		case 54 : 		case 53 : 		case 52 : 		case 51 : 		case 50 : 
		case 49 : 		case 48 :		case 47 :		case 46 :		case 45 : 
		case 44 : 		case 43 :		case 42 :		case 41 :		case 40 : 
		case 39 : 		case 38 :		case 37 :		case 36 :		case 35 : 
		case 34 : 		case 33 :		case 32 :		case 31 :		case 30 : 
		case 29 : 		case 28 :		case 27 :		case 26 :		case 25 : 
		case 24 : 		case 23 :		case 22 :		case 21 :		case 20 : 
		case 19 : 		case 18 :		case 17 :		case 16 :		case 15 : 
		case 14 : 		case 13 :		case 12 :		case 11 :		case 10 : 
		case 9 : 		case 8 :		case 7 :		case 6 :		case 5 : 
		case 4 : 		case 3 :		case 2 :		case 1 : 
		case 0 : result = "불합격"; break;
		default : result = "평균 점수 에러";
		}
		System.out.println("정답 18 : " + result);
		System.out.println("---------------------------------------------");
		
		//-------------------------------------------
		// <정답19> 유식한 방법
		//-------------------------------------------
		int num = (int)avg/10;
		switch(num) {
		case 10 : result = "합격"; break;
		case 9 : result = "합격"; break;
		case 8 : result = "합격"; break;
		case 7 : result = "불합격"; break;
		case 6 : result = "불합격"; break;
		case 5 : result = "불합격"; break;
		case 4 : result = "불합격"; break;
		case 3 : result = "불합격"; break;
		case 2 : result = "불합격"; break;
		case 1 : result = "불합격"; break;
		case 0 : result = "불합격"; break;
		default : result = "평균 점수 에러";
		}
		System.out.println("정답 19 : " + result);
		System.out.println("---------------------------------------------");
		
		//-------------------------------------------
		// <정답20> 매우 유식한 방법
		//-------------------------------------------
		switch(num) {
		case 10 :	case 9 : 
		case 8 : result = "합격"; break;
		case 7 :	case 6 :	case 5 :	case 4 :
		case 3 :	case 2 :	case 1 :
		case 0 : result = "불합격"; break;
		default : result = "평균 점수 에러";
		}
		System.out.println("정답 20 : " + result);
		System.out.println("---------------------------------------------");
		
		
		//-------------------------------------------
		// <문제>
		//-------------------------------------------
		// switch 구문을 사용하여 학점을 도스창에 출력하기
		//-------------------------------------------
		
		//-------------------------------------------
		// <정답21>
		//-------------------------------------------
		switch(num) {
		case 10 :
		case 9 : result = "A"; break;
		case 8 : result = "B"; break;
		case 7 : result = "C"; break;
		case 6 : result = "D"; break;
		case 5 :	case 4 :
		case 3 :	case 2 :	case 1 :
		case 0 : result = "F"; break;
		default : result = "학점 에러";
		}
		System.out.println("정답 21 : " + result);
		System.out.println("---------------------------------------------");
	}

}
