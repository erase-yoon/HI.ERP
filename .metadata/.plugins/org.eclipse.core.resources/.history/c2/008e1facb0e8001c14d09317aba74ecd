public class aaa {
	
	private int emp_no;
	private String emp_name;
	private String emp_loc;
	
	public aaa(int emp_no, String emp_name, String emp_loc) {
		this.emp_no = emp_no;
		this.emp_name = emp_name;
		this.emp_loc = emp_loc;
		
	}
	
	public int getEmp_no() {
		return emp_no;
	}
	
	public void setEmp_no() {
		this.emp_no = emp_no;
	}
	
	public String emp_name() {
		return emp_name;
	}
	
	public void setEmp_name() {
		this.Emp_name = Emp_name;
 	}
	
	
	
	
}
